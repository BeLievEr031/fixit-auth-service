build/
dist/
coverage/
node_modules/

*.spec.ts
eslint.config.mjs
*.json
.prettierrc

# Ignore specific config files or lock files:
package-lock.json
yarn.lock

# Ignore environment files:
.env
.env.local

# Ignore compiled files:
*.min.js
*.min.css
*.bundle.js

# Ignore specific directories:
public/
static/

# Ignore test snapshots:
**/__snapshots__/
**/*.snap

# Ignore files with custom formatting or generated files:
*.generated.ts
*.generated.js

# Ignore documentation files:
docs/**/*.md
README.md
